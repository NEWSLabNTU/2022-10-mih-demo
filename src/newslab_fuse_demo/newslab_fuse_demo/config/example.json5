{
    "version": "0.1.0",
    "namespace": "/",
    "pcd_topic": "velodyne_points",
    "otobrite_img_topic": "otobrite_image",
    "kneron_det_topic": "kneron_detection",

    // LiDAR
    "pcd_roi": {
        "enabled": true,
        "center_xyz": [2.8, 0.0, 0],
        "size_xyz": [0.5, 2.0, 3.0],
        "yaw_degs": 0.0,
